include ../title/title
include ../tick-box/tick-box

mixin tick-boxes()
  section.tick-boxes
    +title("Tick boxes")
    .tick-boxes__row
      .tick-boxes__wrapper
        +tick-box({ name:"radio", isChecked: "true" })
      .tick-boxes__wrapper
        +tick-box({ name:"radio", isChecked: "false" })